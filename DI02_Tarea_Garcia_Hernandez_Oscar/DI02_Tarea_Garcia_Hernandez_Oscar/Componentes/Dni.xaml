<UserControl x:Class="DI02_Tarea_Garcia_Hernandez_Oscar.Componentes.Dni"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:DI02_Tarea_Garcia_Hernandez_Oscar.Componentes"
             mc:Ignorable="d"
             d:DesignHeight="30" d:DesignWidth="200">
    <Grid>
        <Label Name="labelDni" Content="DNI" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,0,0,0"/>
        <TextBox Name="TextBoxDni" Text="{Binding .Numero}" VerticalAlignment="Center" HorizontalAlignment="Center" Margin="0,0,0,0" Width="120" LostFocus="ObtenerNif"/>
    </Grid>
</UserControl>
